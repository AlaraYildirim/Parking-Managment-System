<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="application.GecmisController"
      spacing="10" alignment="CENTER" style="-fx-padding: 20;">
    
    <Label text="Araç Hareket Geçmişi" style="-fx-font-size: 18px;"/>

    <TableView fx:id="historyTable" prefWidth="500">
        <columns>
            <TableColumn fx:id="plateColumn" text="Plaka" prefWidth="150"/>
            <TableColumn fx:id="entryColumn" text="Giriş Zamanı" prefWidth="175"/>
            <TableColumn fx:id="exitColumn" text="Çıkış Zamanı" prefWidth="175"/>
        </columns>
    </TableView>
</VBox>
